{
  "name": "raischburn-ui",
  "theme": "ui",
  "version": "0.0.0",
  "description": "A darker Zenburn like UI theme for Atom.",
  "keywords": [
    "ui",
    "theme",
    "zen",
    "zenburn",
    "dark",
    "night"
  ],
  "repository": "https://github.com/RumpelRaisch/raischburn-ui",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "theme": {
      "title": "UI Theme",
      "order": 1,
      "description": "Select a UI theme. If not all UI parts change color, you should restart Atom.",
      "type": "string",
      "enum": [
        "dark",
        "darker"
      ],
      "default": "dark"
    }
  },
  "main": "./lib/main"
}
